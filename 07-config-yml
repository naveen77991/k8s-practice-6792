apiVersion: v1
kind: ConfigMap 
metadata: 
  name: config 
data: 
  course: aws 
---
apiVersion: v1
kind: Pod 
metadata: 
  name: hellojava 
spec: 
 containers: 
   - name: java 
     image: nginx 
     env: 
       - name: course 
         valueFrom: 
           configMapKeyRef: 
              name: config 
              key: course       